    "import numpy as np\n",
    "from numpy.random import randn\n",
    "n = 100\n",
    "counter = 0\n",
    "for counter in randn(n):\n",
    "    if n > -1 and n < 1:\n",
    "        counter = counter + 1\n",
    "answer = counter/n\n",
    "print(answer)"
